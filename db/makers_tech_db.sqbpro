DROP TABLE IF EXISTS productos;

-- 2. Crear la tabla de productos
CREATE TABLE productos (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre TEXT NOT NULL,
    categoria TEXT NOT NULL,           -- Laptop, Desktop, Procesador, etc.
    subcategoria TEXT,                 -- Más recomendado, Recomendado, Menos recomendado
    marca TEXT,                        -- Extraída del nombre (ej: ASUS, Dell, HP)
    cpu TEXT,                          -- Procesador
    gpu TEXT,                          -- Tarjeta gráfica
    ram TEXT,                          -- Memoria RAM
    almacenamiento TEXT,               -- SSD, HDD, NVMe
    precio INTEGER NOT NULL,           -- Precio en COP (sin comillas ni símbolos)
    stock INTEGER DEFAULT 5,           -- Stock simulado (puedes actualizarlo después)
    recomendacion_nivel TEXT,          -- &quot;Alta&quot;, &quot;Media&quot;, &quot;Baja&quot;
    recomendacion_stars INTEGER,       -- 2, 4, 5 estrellas
    uso_recomendado TEXT               -- Estudiantes, Gaming, Oficina, etc.
);

